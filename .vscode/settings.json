{
  "i18n-ally.localesPaths": [
    "src/i18n"
  ],
  "i18n-ally.keystyle": "nested",
  "eslint.validate": [
    "javascript",
    "typescript",
    "svelte",
    "json"
  ],
  "typescript.preferences.importModuleSpecifier": "relative",
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[svelte]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
}